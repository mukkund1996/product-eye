# Browserbase API Configuration (required for StagehandTool)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# LLM and Service Configuration
MODEL=gpt-4o-mini
OPENAI_API_KEY=YOUR_API_KEY
GROQ_API_KEY=YOUR_API_KEY
GEMINI_API_KEY=YOUR_API_KEY

# Memory Configuration
# Set to "true" to use OpenAI RAG storage for memory (requires valid OpenAI API key with sufficient credits)
# Set to "false" to use local memory storage (fallback)
USE_OPENAI_MEMORY=false

# CrewAI Tracing Configuration
CREWAI_TRACING_ENABLED=true
